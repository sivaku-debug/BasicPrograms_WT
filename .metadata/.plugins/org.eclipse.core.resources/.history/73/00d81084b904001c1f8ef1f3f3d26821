<script type="text/javascript">
function forloopEx(){
	document.write("For loop output")
	for(i=1;i<=5;i++){
		document.write(i+"<br\>");
	}
}

function whileLoopEx(){
	document.write("while loop example");
	var i=1;
	while(i<=5){
		document.write(i+"<br\>");
	}
	i++;
}





forloopEx();


</script>
